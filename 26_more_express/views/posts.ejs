<% include partials/header %>

<h1>The post page</h1>
<!-- loop 1 -->
<% for (var i = 0; i < posts.length; i++) { %>
    <li>
        Title: <%= posts[i].title %> - 
        <strong><%= posts[i].author %></strong>
    </li>
<% } %>
<!-- loop 2 -->
<% posts.forEach(function(post) { %>
    <li>
        Title: <%= post.title %> - 
        <strong><%= post.author %></strong>
    </li>
<% }) %>

<% include partials/footer %>